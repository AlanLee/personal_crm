<p id="notice"><%= notice %></p>

<h1>Dinner Friends</h1>

<table>
  <thead>
    <tr>
      <th>Who</th>
      <th>Contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dinner_friends.each do |dinner_friend| %>
      <tr>
        <td><%= dinner_friend.who %></td>
        <td><%= dinner_friend.contact %></td>
        <td><%= link_to 'Show', dinner_friend %></td>
        <td><%= link_to 'Edit', edit_dinner_friend_path(dinner_friend) %></td>
        <td><%= link_to 'Destroy', dinner_friend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dinner Friend', new_dinner_friend_path %>
